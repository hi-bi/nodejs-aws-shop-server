{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "destroy": "cdk destroy -f --all",
    "deployprof": "cdk deploy --profile admin --all",
    "destroyprof": "cdk destroy -f --profile admin --all",
    "lambda-p": "ts-node bin/lambda-build.ts",
    "data": "ts-node scripts/put-products.ts",
    "lambda-a": "ts-node bin/lambda-auth-build.ts",
    "lambda-i": "ts-node bin/lambda-ipf-build.ts",
    "deploy-p": "cdk deploy ProductServiceStack-711",
    "destroy-p": "cdk destroy -f ProductServiceStack-711",
    "deploy-a": "cdk deploy AuthorizationServiceStack",
    "destroy-a": "cdk destroy -f AuthorizationServiceStack",
    "deploy-i": "cdk deploy ImportServiceStack-711",
    "destroy-i": "cdk destroy -f ImportServiceStack-711"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "aws-cdk": "2.145.0",
    "esbuild": "0.21.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.7",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.2.0",
    "@aws-sdk/client-dynamodb": "^3.602.0",
    "@aws-sdk/client-s3": "^3.600.0",
    "@aws-sdk/client-sns": "^3.609.0",
    "@aws-sdk/client-sqs": "^3.609.0",
    "@aws-sdk/lib-dynamodb": "^3.602.0",
    "@aws-sdk/s3-request-presigner": "^3.600.0",
    "@types/aws-lambda": "^8.10.140",
    "aws-cdk-lib": "2.145.0",
    "aws-sdk-client-mock": "^4.0.1",
    "aws-sdk-client-mock-jest": "^4.0.1",
    "constructs": "^10.0.0",
    "csv-parser": "^3.0.0",
    "source-map-support": "^0.5.21",
    "yup": "^1.4.0"
  }
}
